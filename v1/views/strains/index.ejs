<% include ../partials/header %>
    
    <div class = "ui inverted segment container">
        <% strains.forEach(function(strain){%>
            <div class="ui internally celled grid">
                <div class="row">
                    <div class="three wide column">
                        <a href="http://google.com" class="ui small image">
                            <img src = "<%= strain.image%>">
                        </a>
                    </div>
                    <div class="ten wide column">
                        <div>
                            <p><strong><%= strain.name%></strong></p>
                        </div>
                        <p><%= strain.description%></p>
                    </div>
                    <div class="three wide column">
                        <p id = "strain-author"><em>Submitted by: <%= strain.author%></em></p>        
                    </div>
                </div>
            </div>
            <hr>
        <% });%>
    </div>

<% include ../partials/footer %>